            <string>if mouse_check_button_pressed(mb_left) {
    changekeys = true;
}

if changekeys = true {
    ini_open("Settings.ini");
    if keyboard_check_pressed(vk_anykey) &amp;&amp; key = "UP" {
        global.up = keyboard_lastkey
        global.uptext = global.up
        ini_write_real("Controls", "Up", global.up)
        key = "DOWN"
        exit;
    }
    if keyboard_check_pressed(vk_anykey) &amp;&amp; key = "DOWN" {
        global.down = keyboard_lastkey
        global.downtext = global.down
        ini_write_real("Controls", "Down", global.down)
        key = "LEFT"
        exit;
    }
    if keyboard_check_pressed(vk_anykey) &amp;&amp; key = "LEFT" {
        global.left = keyboard_lastkey
        global.lefttext = global.left
        ini_write_real("Controls", "Left", global.left)
        key = "RIGHT"
        exit;
    }
    if keyboard_check_pressed(vk_anykey) &amp;&amp; key = "RIGHT" {
        global.right = keyboard_lastkey
        global.righttext = global.right
        ini_write_real("Controls", "Right", global.right)
        key = "PICKUP ITEM"
        exit;
    }
    if keyboard_check_pressed(vk_anykey) &amp;&amp; key = "PICKUP ITEM" {
        global.pickup = keyboard_lastkey
        global.pickuptext = global.pickup
        ini_write_real("Controls", "PickupItem", global.pickup)
        key = "DONE!"
        if alarm[0] = -1 {
        alarm[0] = 1.5*room_speed
        }
        ini_close();
        exit;
    }
}
