            <string>/*****************************
* If a command has been submitted...
*****************************/
if console_submit(my_console) {

    /*****************************
    * Here we can put all our commands.
    *****************************/
    
    /*****************************
    * If user enters into the console:
    * show_message "message"
    *****************************/
    if console_cmd(my_console,"show_message") {
    
        /*****************************
        * Get the message value to display
        *****************************/
        msg = console_value(my_console,1);
        
        /*****************************
        * Show message
        *****************************/
        show_message(msg);
    
    }
    
    if instance_exists(obj_player) {
        if console_cmd(my_console,"Hat") {
            show_debug_message(obj_player.hat);
        }
        
        if console_cmd(my_console,"Hp") {
            if console_value_count(my_console) = 1 {
                obj_player.hp = console_value(my_console,1);
            } else
            if console_value_count(my_console) = 0 {
                    show_debug_message(obj_player.hp);
            }
        }
        
        if console_cmd(my_console,"Durability") {
            if console_value_count(my_console) = 1 {
                    obj_player.durability = console_value(my_console,1);
            } else
            if console_value_count(my_console) = 0 {
                    show_debug_message(obj_player.durability);
            }
        }
    } else {
        show_debug_message("Player not found!");
    }
}
</string>
